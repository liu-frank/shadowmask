{
  "swagger": "2.0",
  "info": {
    "title": "WebApi",
    "description": "apis for webUI",
    "version": "1.0.0"
  },
  "host": "127.0.0.1",
  "schemes": [
    "https"
  ],
  "basePath": "/",
  "produces": [
    "application/json"
  ],
  "paths": {
    "/admin/login": {
      "post": {
        "summary": "Adminstrator login api",
        "parameters": [
          {
            "name": "username",
            "in": "formData",
            "description": "Adminstrator username",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "type": "string",
            "description": "password for Adminstrator",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "return log result",
            "schema": {
              "$ref": "#/definitions/LoginResult"
            }
          }
        }
      }
    },
    "/data/schema": {
      "get": {
        "summary": "Adminstrator login api",
        "parameters": [
          {
            "name": "source",
            "in": "formData",
            "description": "hive,spark,etc",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "return the schema data",
            "schema": {
              "$ref": "#/definitions/SchemaResult"
            }
          }
        }
      }
    },
    "/data/table": {
      "get": {
        "summary": "get n-first record of a table",
        "parameters": [
          {
            "name": "table",
            "in": "formData",
            "description": "table name.",
            "required": true,
            "type": "string"
          },
          {
            "name": "rows",
            "in": "formData",
            "description": "how many rows will get",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "return fetch results .",
            "schema": {
              "$ref": "#/definitions/TableResult"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "LoginResult": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "info": {
          "type": "string"
        },
        "data": {
          "$ref": "#/definitions/LoginResultData"
        }
      }
    },
    "LoginResultData": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string"
        }
      }
    },
    "SchemaResult": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "info": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SchemaObject"
          }
        }
      }
    },
    "SchemaObject": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "showName": {
          "type": "string"
        },
        "tables": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TableProp"
          }
        }
      }
    },
    "TableProp": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "showName": {
          "type": "string"
        }
      }
    },
    "TableResult": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "info": {
          "type": "string"
        },
        "data": {
          "$ref": "#/definitions/TableContent"
        }
      }
    },
    "TableContent": {
      "type": "object",
      "properties": {
        "titles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TableTitle"
          }
        },
        "data": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "TableTitle": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "showName": {
          "type": "string"
        }
      }
    }
  }
}